{
  "name": "flare-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 80 --host",
    "test": "vitest --run",
    "test:ui": "vitest",
    "test:watch": "vitest --watch",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui --project=chromium --project=auth-flow",
    "e2e:ui:all": "playwright test --ui --project=setup --project=chromium --project=auth-flow",
    "e2e:login": "playwright test --project=setup"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.11",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^25.0.1",
    "vitest": "^2.1.3",
    "vite": "^5.4.9",
    "@playwright/test": "^1.48.2"
  }
}
